{
  "manifest_version": 3,
  "name": "Search Result Filter",
  "description": "__MSG_extDesc__",
  "version": "1.5.1",
  "default_locale": "en",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*",
        "https://www.bing.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["_locales/*/messages.json"],
      "matches": ["<all_urls>"]
    }
  ]
}
